{
    "extends": "airbnb",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "max-len": [
            "error",
            120
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "react/prop-types": 0,
        "react/no-did-update-set-state": 0,
        "import/no-dynamic-require": 0
    }
}
